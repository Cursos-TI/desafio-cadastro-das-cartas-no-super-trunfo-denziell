#include <stdio.h>

int main() {
    //codigo do estado
    char codigo[50];
    //nome da cidade//
    char cidade[50];
    //populaçao da cidade//
    float populaçao;
    //area em k² da cidade//
    float km²;
    //pib da cidade//
    float pib;
    //numero de pontos turisticos//
    int turisticos;

    printf("riodejaneiro\n");
    
    printf("digite o codigo da cidade\n");
    scanf("%d", &codigo);
    
    printf("digite o nome da cidade\n");
    scanf("%s", &cidade);

    printf("digite a populaçao da cidade\n");
    scanf("%f", &populaçao);

    printf("digite a area em k² da cidade\n");
    scanf("%f", &km²);

    printf("digite o pib da cidade\n");
    scanf("%f", &pib);

    printf("digite quantos pontos turistico tem na cidade\n");
    scanf("%d", &turisticos);
    
    printf("saopaulo\n");
    
    printf("digite o codigo da cidade 2\n");
    scanf("%d", &codigo);
    
    printf("digite o nome da cidade 2\n");
    scanf("%s", &cidade);

    printf("digite a populaçao da cidade 2\n");
    scanf("%f", &populaçao);

    printf("digite a area em k² da cidade 2\n");
    scanf("%f", &km²);

    printf("digite o pib da cidade 2\n");
    scanf("%f", &pib);

    printf("digite quantos pontos turistico tem na cidade 2\n");
    scanf("%d", &turisticos);
    

    return 0;


  }
